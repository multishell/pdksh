echo $ac_n "checking if you have /dev/fd/n""... $ac_c" 1>&6
  if eval "test \"`echo '$''{'ksh_cv_dev_fd'+set}'`\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
